# Firebase (required for auth, Firestore, storage)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# Face verification for matrimonial photos (required for create matrimonial profile)
EXPO_PUBLIC_HUGGING_FACE_TOKEN=

# Optional: Sign in with Google (see docs/GOOGLE_SIGNIN_SETUP.md)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# Optional: Instagram feed integration (leave empty to skip)
EXPO_PUBLIC_INSTAGRAM_ACCESS_TOKEN=

# Optional: URL to docs for "View docs online" in About screen (e.g. GitHub docs folder)
# EXPO_PUBLIC_DOCS_BASE_URL=https://github.com/your-org/Bhatia-Buzz/tree/main/docs

# Optional: Contact emails (defaults used if not set)
# EXPO_PUBLIC_PRIVACY_EMAIL=privacy@bhatiabuzz.com
# EXPO_PUBLIC_LEGAL_EMAIL=legal@bhatiabuzz.com
# EXPO_PUBLIC_SUPPORT_EMAIL=support@bhatiabuzz.com

# Optional: Privacy policy and terms URLs (in-app "View online" + App Store / Play Store listing)
# EXPO_PUBLIC_PRIVACY_POLICY_URL=https://yoursite.com/privacy
# EXPO_PUBLIC_TERMS_OF_SERVICE_URL=https://yoursite.com/terms
